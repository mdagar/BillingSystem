<script>
    $(document).ready(function () {
        //set the initial body width
        var originalWidth = 400;
        debugger;
        var totalCount = "@Model.BillLines.Count";
        originalWidth = originalWidth - totalCount * 15;

        $(".minHeight").css("min-height", originalWidth);

        document.getElementById("ItemPreview").src = "data:image/png;base64," + "@Convert.ToBase64String(Model.CompanyLogo, 0, Model.CompanyLogo.Length)";//"@Model.CompanyLogo";

    });

    function printDiv() {
        var divToPrint = document.getElementById('DivIdToPrint');

        var newWin = window.open('', 'Print-Window');
        newWin.document.open();
        newWin.document.write('<html><body onload="window.print()"><div>Original Invoice</div>' + divToPrint.innerHTML + "<div style='page-break-after: always;'></div><div>Buyer Invoice</div>" + divToPrint.innerHTML + "<div style='page-break-after: always;'></div><div>Extra Invoice</div>" + divToPrint.innerHTML + '</body></html>');
        newWin.document.close();
        setTimeout(function () { newWin.close(); }, 10);
    }

</script>
